{
  "name": "@atzomx/ui",
  "version": "0.0.0",
  "description": "UI Components for React",
  "scripts": {
    "storybook:run": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest --env=jsdom",
    "copy-files": "node ./scripts/copy-files.js",
    "build": "yarn clean && tsc -P ./tsconfig.json",
    "clean": "rimraf build/",
    "postpublish": "yarn revert:bundle",
    "prepublishOnly": "yarn build && yarn copy-files && cp -r ./build/* .  && rm -rf ./build",
    "revert:bundle": "git clean -fd"
  },
  "engines": {
    "yarn": "1.22.4"
  },
  "module": "./src/index.ts",
  "main": "./src/index.ts",
  "types": "./src/index.d.ts",
  "repository": "https://github.com/atzomx/ui",
  "author": "SaulMoreyra",
  "license": "MIT",
  "devDependencies": {
    "@emotion/jest": "11.8.0",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@react-theming/storybook-addon": "1.1.5",
    "@storybook/addon-actions": "6.4.12",
    "@storybook/addon-docs": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.4.12",
    "@storybook/react": "6.1.21",
    "@storybook/storybook-deployer": "2.8.10",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.8",
    "@types/react": "18.0.8",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "git-commit-msg-linter": "^4.1.2",
    "jest": "27.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {}
}
